{
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "template": "node_modules/@escommunity/minami"
    },
    "source": {
        "include": [
            "index.js",
            "./src/Contact.js",
            "./src/Documentation.js",
            "./src/index.js",
            "./src/License.js",
            "./src/Servers.js"
        ],
        "exclude": [
            "node_modules",
            "client"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    }
}
